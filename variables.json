{
  "name": "knowage",
  "trademark": "Knowage",
  "compose_file": "docker-compose.yml",
  "installpath": "/data/wwwroot/knowage",
  "env_file": ".env",
  "fork_url": "https://github.com/KnowageLabs/Knowage-Server-Docker",
  "requirements":{
    "cpu_arch": "Linux x86-64, ARM 32/64, Windows x86-64, IBM POWER8, x86/i686",
    "cpu": "1",
    "memory": "4",
    "disk": "20",
    "swap": "2",
    "url": "https://knowage-suite.readthedocs.io/en/7.4/installation-guide/knowage-ce-installer.html#server-side-requirements"
  },

  "user": {
    "username": "biadmin",
    "password": "biadmin"
  },
  "services":[
    {
      "name": "knowage",
      "port": "9001",
      "description_cn": "浏览器访问 Knowage",
      "description_en": "Browser access to Knowage by Http",
      "required": "Y"
    },
    {
      "name": "mariadb",
      "port": "3306",
      "description_cn": "TCP 访问MariaDB数据库",
      "description_en": "Accessing MySQL database with TCP",
      "required": "Y"
    }
  ]

}